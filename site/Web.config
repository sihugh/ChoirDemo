<?xml version="1.0"?>
<configuration>
    <system.web>
      <customErrors mode="On" />
      <httpRuntime enableVersionHeader="false" />
    </system.web>
  <system.webServer>
    <security>
      <requestFiltering removeServerHeader="true" />
    </security>
    <httpProtocol>
      <customHeaders>
        <remove name="X-Powered-By" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>
</configuration>
